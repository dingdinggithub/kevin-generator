<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE entity SYSTEM "../dtd/table.dtd">
<!--
name:类名称,首字母大写
package:包名,方面后面的扩展(eg：com.zs.xybb.domian)
description：（类的中文名称）页面中用到
author：类的开发作者（注释会用到）
version:类的开发版本
 -->
<entity name="ComplaintsInfo" table="Complaints_Info" description="投诉举报" author="刘伟伟" since="1.0">
	<!-- 
		主键
	 -->
	<id name="id" column="id" type="int" description="主键"/>
	<!-- 
		name:表示属性名称
		type：属性类型,int | long | string | date 选择其中一个
		description:描述,生成注释需要，页面内容
	 -->
	<property name="acceptanceUnit" column="acceptance_unit" type="string" description="受理单位"/>
	<property name="complaintsMode" column="complaints_mode" type="string" description="诉讼方式"/>
	<property name="complainantName" column="complainant_name" type="string" description="诉讼方姓名"/>
	<property name="complainantPhone" column="complainant_phone" type="string" description=" 诉讼方联系电话"/>
	<property name="inputDate" column="input_date" type="string" description="诉讼方地址"/>
	
	<property name="remark" column="remark" type="string" description="备注"/>
	<property name="defendantName" column="defendant_name" type="string" description="被诉方姓名"/>
	<property name="defendantPhone" column="defendant_phone" type="string" description="被诉方联系电话"/>
	<property name="defendantAddress" column="defendant_address" type="string" description="被诉方地址"/>
	<property name="officer" column="officer" type="string" description="负责人"/>
	
	<property name="ComplaintType" column="Complaint_type" type="string" description="诉讼类别"/>
	<property name="consumDate" column="consum_date" type="date" description=" 消费日期"/>
	<property name="priceQty" column="price_qty" type="string" description="价格数量"/>
	<property name="actionDate" column="action_date" type="date" description="诉讼日期"/>
	<property name="consumerCertificate" column="consumer_certificate" type="string" description="消费凭证"/>
	
	
	<property name="caseIntroduction" column="case_introduction" type="string" description="情况介绍"/>
	<property name="opinions" column="opinions" type="string" description="受理意见"/>
	<property name="process" column="process" type="string" description="受理过程"/>
	<property name="lossAmount" column="loss_amount" type="string" description="免受损失金额单位（元）"/>
	<property name="result" column="result" type="int" description="受理结果"/>
</entity>