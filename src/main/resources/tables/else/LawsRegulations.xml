<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE entity SYSTEM "../dtd/table.dtd">
<!--
name:类名称,首字母大写
package:包名,方面后面的扩展(eg：com.zs.xybb.domian)
description：（类的中文名称）页面中用到
author：类的开发作者（注释会用到）
version:类的开发版本
 -->
<entity name="LawsRegulations" table="laws_regulations" description="法律法规管理表" author="刘伟伟" since="1.0">
	<!-- 
		主键
	 -->
	<id name="id" column="id" type="int" description="主键"/>
	<!-- 
		name:表示属性名称
		type：属性类型,int | long | string | date 选择其中一个
		description:描述,生成注释需要，页面内容
	 -->
	<property name="title" column="title" type="string" description="标题"/>
	<property name="text" column="text" type="string" description="内容"/>
	<property name="publishDate" column="publish_date" type="date" description="发布时间"/>
	<property name="status" column="status" type="int" description="状态"/>
</entity>