<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE entity SYSTEM "../dtd/table.dtd">
<!--
name:类名称,首字母大写
package:包名,方面后面的扩展(eg：com.zs.xybb.domian)
description：（类的中文名称）页面中用到
author：类的开发作者（注释会用到）
version:类的开发版本
 -->
<entity name="Permissions" table="sys_permissions" description="权限" author="余进" since="1.0">
	<!-- 
		主键
	 -->
	<id name="id" column="id" type="string" description="主键"/>
	<!-- 
		name:表示属性名称
		type：属性类型,int | long | string | date 选择其中一个
		description:描述,生成注释需要，页面内容
	 -->
	<property name="name" column="name" type="string" description="权限名称"/>
	<property name="code" column="code" type="string" description="权限码"/>
	<property name="pcode" column="pcode" type="string" description="树形码"/>
	<property name="url" column="url" type="string" description="URL"/>
	<property name="status" column="status" type="int" description="权限状态"/>
	<property name="type" column="type" type="int" description="权限类型"/>
</entity>