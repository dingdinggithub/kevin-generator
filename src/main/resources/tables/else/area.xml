<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE entity SYSTEM "../dtd/table.dtd">
<!--
name:类名称,首字母大写
package:包名,方面后面的扩展(eg：com.zs.xybb.domian)
description：（类的中文名称）页面中用到
author：类的开发作者（注释会用到）
version:类的开发版本
 -->
<entity name="AreaInfo" table="area_info" description="行政区划表" author="fyjiang" since="1.0">
	<!-- 
		主键
	 -->
	<id name="id" column="id" type="string" description="主键"/>
	<!-- 
		name:表示属性名称
		type：属性类型,int | long | string | date |decimal选择其中一个
		description:描述,生成注释需要，页面内容
	 -->
	
<property name="areaName"    column="area_name"      type="string"     maxlength="50"  description="地区名"   />
<property name="pid"    column="pid"      type="string"     maxlength="20"  description="父ID"   />
<property name="lev"    column="lev"      type="int"   description="区域级别"   />
<property name="status"    column="status"      type="int"    description="状态"   />

	
</entity>
