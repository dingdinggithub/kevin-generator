<?xml version="1.0" encoding="UTF-8"?>                                          
<!DOCTYPE entity SYSTEM "../dtd/table.dtd">
<!--                                                                            
name:类名称,首字母大写                                                          
package:包名,方面后面的扩展(eg：com.zs.xybb.domian)                             
description：（类的中文名称）页面中用到                                         
author：类的开发作者（注释会用到）                                              
version:类的开发版本                                                            
 -->                                                                            
<entity name="bfdw" table="bfdw" description="帮扶单位表" author="孙强强" since="1.0">
	<!--                                                                          
		主键                                                                        
	 -->                                                                          
	<id name="id" column="id" type="string" description="主键"/>                  
	<!--                                                                          
		name:表示属性名称                                                           
		type：属性类型,int | long | string | date |decimal选择其中一个              
		description:描述,生成注释需要，页面内容                                     
	 -->
<property	name="BFBH"  		 column="BFBH" type="string"  maxlength="255"      description="帮扶编号" />
<property	name="JTBH"  		 column="JTBH" type="string"  maxlength="18"     description="家庭编号"   />
<property	name="BFDW"  		 column="BFDW" type="string"  maxlength="50"     description="帮扶单位"   />
<property	name="XM"      		 column="XM" type="string"  maxlength="30"   description="姓名"     />
<property	name="ZW"     		 column="ZW" type="string"  maxlength="20"   description="职务"     />
<property	name="XB"      		 column="XB" type="string"  maxlength="2"   description="性别"      />
<property	name="SFZH"  		 column="SFZH" type="string"  maxlength="18"   description="身份证号"     />
<property	name="ZZMM"  		 column="ZZMM" type="string"  maxlength="20"   description="政治面貌"     />
<property	name="BFKSSJ"  column="BFKSSJ" type="date"     description="帮扶开始时间" dateFmt="yyyy-MM-dd"    />
<property	name="BFTPSJ"  column="BFTPSJ" type="date"    description="帮扶脱贫时间"  dateFmt="yyyy-MM-dd"   />
<property	name="ZYBFCS"  column="ZYBFCS" type="string"  maxlength="255"   description="主要帮扶措施"    />
<property	name="LXDH"   	 column="LXDH" type="string"  maxlength="20"   description="联系电话"     />
<property name="inputTime" column="input_time" type="date" maxlength="0" description="录入时间" dateFmt="yyyy-MM-dd HH:mm:ss"/> 
<property name="inputUser" column="input_user" type="string" maxlength="255" description="录入人"  />

</entity>                                                                                          












