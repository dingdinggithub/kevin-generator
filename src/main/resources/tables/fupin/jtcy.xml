<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE entity SYSTEM "../../dtd/table.dtd">
<!--
name:类名称,首字母大写
package:包名,方面后面的扩展(eg：com.zs.xybb.domian)
description：（类的中文名称）页面中用到
author：类的开发作者（注释会用到）
version:类的开发版本
 -->
<entity name="jtcy" table="jtcy" description="家庭成员表" author="孙强强" since="1.0">
	<!-- 
		主键
	 -->
	<id name="id" column="id" type="string" description="主键"/>
	<!-- 
		name:表示属性名称
		type：属性类型,int | long | string | date |decimal选择其中一个
		description:描述,生成注释需要，页面内容
	 -->
	 <property name="JTBH" column="JTBH" type="string" maxlength="50" description="家庭编号" />
	 <property name="RYXM" column="RYXM" type="string" maxlength="20" description="人员姓名" />
	 <property name="RYXB" column="RYXB" type="string" maxlength="2" description="人员性别" />
	 <property name="CSNY" column="CSNY" type="date"  description="出生年月" dateFmt="yyyy-MM-dd" />
	 <property name="SFZH" column="SFZH" type="string" maxlength="20" description="身份证号" />
	 <property name="RYMZ" column="RYMZ" type="string" maxlength="20" description="人员民族" />
	 <property name="ZZMM" column="ZZMM" type="string" maxlength="20" description="政治面貌" />
	 <property name="WHCD" column="WHCD" type="string" maxlength="20" description="文化程度" />
	 <property name="ZXSCQ" column="ZXSCQ" type="string" maxlength="20" description="在校生情况" />
	 <property name="JKZK" column="JKZK" type="string" maxlength="20" description="健康状况" />
	 <property name="LDJN" column="LDJN" type="string" maxlength="50" description="劳动技能" />
	 <property name="WGQK" column="WGQK" type="string" maxlength="50" description="务工情况" />
	 <property name="DNWZSJ" column="DNWZSJ" type="string" maxlength="50" description="当年务工时间" />
	 <property name="NWGSR" column="NWGSR" type="string" maxlength="50" description="年务工收入" />
	 <property name="SFXYJR" column="SFXYJR" type="string" maxlength="2" description="是否现役军人" />
	 <property name="SFCJHL" column="SFCJHL" type="string" maxlength="2" description="是否参加合疗" />
	 <property name="SFDBYB" column="SFDBYB" type="string" maxlength="50" description="是否大病医保" />
	 <property name="ZP" column="ZP" type="string" maxlength="50" description="照片" />
	 <property name="inputTime" column="input_time" type="date" maxlength="0" description="录入时间" dateFmt="yyyy-MM-dd HH:mm:ss" />
	 <property name="inputUser" column="input_user" type="string" maxlength="255" description="录入人" />
	 
	 
	 
	 </entity>