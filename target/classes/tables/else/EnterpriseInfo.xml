<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE entity SYSTEM "../dtd/table.dtd">
<!--
name:类名称,首字母大写
package:包名,方面后面的扩展(eg：com.zs.xybb.domian)
description：（类的中文名称）页面中用到
author：类的开发作者（注释会用到）
version:类的开发版本
 -->
<entity name="EnterpriseInfo" table="enterprise_info" description="企业信息管理" author="赵付录" since="1.0">
	<!-- 
		主键
	 -->
	<id name="id" column="id" type="int" description="主键"/>
	<!-- 
		name:表示属性名称
		type：属性类型,int | long | string | date 选择其中一个
		description:描述,生成注释需要，页面内容
	 -->
	<property name="registerNo" column="register_no" type="string" description="注册号"/>
	<property name="enterpriseName" column="enterprise_name" type="string" description="企业名称"/>
	<property name="name" column="name" type="string" description="字号"/>
	<property name="legalPerson" column="legal_person" type="string" description="代表人"/>
	<property name="address" column="address" type="string" description="地址"/>
	<property name="postcode" column="postcode" type="string" description="邮政编码"/>
	<property name="enterpriseType" column="enterprise_type" type="string" description="企业类型"/>
	<property name="economicNature" column="economic_nature" type="string" description="企业经济性质"/>
	<property name="manageOrg" column="manage_org" type="string" description="管辖单位"/>
	<property name="registerOrg" column="register_org" type="string" description="登记机关"/>
	<property name="contact" column="contact" type="string" description="联系电话"/>
	<property name="status" column="status" type="int" description="企业状态"/>
	<property name="approveDate" column="approve_date" type="date" description="核准日期"/>
	<property name="registerCapital" column="register_capital" type="string" description="注册资本(万元)"/>
	<property name="businessScope" column="business_scope" type="string" description="经营范围"/>
	<property name="setupdate" column="setupdate" type="date" description="成立日期"/>
	<property name="maturity" column="maturity" type="date" description="到期日"/>
	<property name="employeeNumber" column="employee_number" type="int" description="从业人数"/>
	<property name="industryType" column="industry_type" type="string" description="行业门类"/>
	<property name="industryCode" column="industry_code" type="string" description="行业代码"/>
	<property name="docketNo" column="docket_no" type="string" description="档案号"/>
	<property name="belongOrg" column="belong_org" type="string" description="属地管理机关"/>
	<property name="investorSituation" column="investor_situation" type="string" description="投资者情况"/>
	<property name="investmentNumber" column="investment_number" type="int" description="投资人数"/>
</entity>