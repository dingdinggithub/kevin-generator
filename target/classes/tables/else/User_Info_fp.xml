<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE entity SYSTEM "../dtd/table.dtd">
<!--
name:类名称,首字母大写
package:包名,方面后面的扩展(eg：com.zs.xybb.domian)
description：（类的中文名称）页面中用到
author：类的开发作者（注释会用到）
version:类的开发版本
 -->
<entity name="UserInfo" table="user_info" description="用户" author="fyjiang" since="1.0">
	<!-- 
		主键
	 -->
	<id name="id" column="id" type="int" description="主键"/>
	<!-- 
		name:表示属性名称
		type：属性类型,int | long | string | date 选择其中一个
		description:描述,生成注释需要，页面内容
	 -->
	<property name="username" column="username" type="string" description="用户名"/>
	<property name="password" column="password" type="string" description="用户密码"/>
	<property name="phone" column="phone" type="string" description="手机号"/>
	<property name="type" column="type" type="int" description="用户类型"/>
	<property name="remark" column="remark" type="string" description="备注"/>
</entity>