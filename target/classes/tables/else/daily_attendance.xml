<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE entity SYSTEM "../dtd/table.dtd">
<!--
name:类名称,首字母大写
package:包名,方面后面的扩展(eg：com.zs.xybb.domian)
description：（类的中文名称）页面中用到
author：类的开发作者（注释会用到）
version:类的开发版本
 -->
<entity name="DailyAttendance" table="daily_attendance" description="日常考勤" author="刘伟伟" since="1.0">
	<!-- 
		主键
	 -->
	<id name="id" column="id" type="int" description="主键"/>
	<!-- 
		name:表示属性名称
		type：属性类型,int | long | string | date 选择其中一个
		description:描述,生成注释需要，页面内容
	 -->
	<property name="department" column="department" type="string" description="所属部门"/>
	<property name="userId" column="user_id" type="string" description="用户id"/>
	<property name="time" column="time" type="date" description="时间"/>
	<property name="state" column="state" type="int" description="状态"/>
	<property name="longitude" column="longitude" type="string" description="经度"/>
	<property name="latitude" column="latitude" type="string" description="纬度"/>
	<property name="distance" column="distance" type="int" description="距离"/>
</entity>	