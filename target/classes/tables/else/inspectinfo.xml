	<property name="mainInfo" column="main_info" type="string" description="检查主要内容"/>
	<property name="measures" column="measures" type="int" description="处理措施"/>
	<property name="inspectConclusion" column="inspect_conclusion" type="string" description="检查结论"/>
	<property name="checkPoints" column="check_points" type="string" description="药品经营企业检查项"/>
	<property name="remark" column="remark" type="string" description="备注"/>
	<property name="checkResult" column="check_result" type="int" description="检查结果"/>
	<property name="checkContents" column="check_contents" type="string" description="检查内容"/>
</entity>