	<property name="inspectInfo" column="inspect_info" type="string" description="企业巡查信息"/>
	<property name="escortUnit" column="escort_unit" type="string" description="单位陪同人员"/>
	<property name="mainProblem" column="main_problem" type="string" description="主要问题"/>
