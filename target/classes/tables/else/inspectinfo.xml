<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE entity SYSTEM "../dtd/table.dtd">
<!--
name:类名称,首字母大写
package:包名,方面后面的扩展(eg：com.zs.xybb.domian)
description：（类的中文名称）页面中用到
author：类的开发作者（注释会用到）
version:类的开发版本
 -->
<entity name="InspectInfo" table="inspect_info" description="企业巡查信息" author="侯艳松" since="1.0">
	<!-- 
		主键
	 -->
	<id name="id" column="id" type="int" description="主键"/>
	<!-- 
		name:表示属性名称
		type：属性类型,int | long | string | date 选择其中一个
		description:描述,生成注释需要，页面内容
	 -->
	<property name="examinee" column="examinee" type="string" description="被检查人"/>
	<property name="legalPerson" column="legal_person" type="string" description="企业法人"/>
	<property name="businessAddress" column="business_address" type="string" description="经营地址"/>
	<property name="registerNo" column="register_no" type="date" description="注册号	"/>
	<property name="licenseNo" column="license_no" type="string" description="许可证号"/>
	<property name="businessScope" column="business_scope" type="string" description="经营范围"/>
	<property name="inspectAddress" column="inspect_address" type="string" description="巡检地址"/>
	<property name="inspectRecord" column="inspect_record" type="string" description="监督检查记录"/>
	<property name="inspectType" column="inspect_type" type="int" description="检查类别"/>
	<property name="inspector" column="inspector" type="string" description="巡检人员"/>
	<property name="fieldCondition" column="field_condition" type="string" description="现场情况"/>
	<property name="educationContent" column="education_content" type="string" description="法律宣传教育内容"/>
	<property name="handleSituation" column="handle_situation" type="string" description="处理情况"/>
