<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE entity SYSTEM "../dtd/table.dtd">
<!--
name:类名称,首字母大写
package:包名,方面后面的扩展(eg：com.zs.xybb.domian)
description：（类的中文名称）页面中用到
author：类的开发作者（注释会用到）
version:类的开发版本
 -->
<entity name="PublicService" table="public_service" description="公共服务" author="胡亚茹" since="1.0">
	<!-- 
		主键
	 -->
	<id name="id" column="id" type="int" description="主键"/>
	<!-- 
		name:表示属性名称
		type：属性类型,int | long | string | date 选择其中一个
		description:描述,生成注释需要，页面内容
	 -->
	<property name="name" column="name" type="string" description="服务名称"/>
	<property name="imageAddress" column="image_address" type="string" description="图片地址"/>
	<property name="link" column="link" type="string" description="链接"/>
	<property name="inputDate" column="input_date" type="date" description="录入时间"/>
</entity>