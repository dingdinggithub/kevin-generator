	<property	name="createBy"   column="create_by" type="string"  maxlength="40"   description="创建人"  />
	<property	name="updateBy"   column="update_by" type="string"  maxlength="40"   description="更新人"  />
	<property	name="createTime"   column="create_time" type="date" description="创建时间"  />
	<property	name="updateTime"   column="update_time" type="date" description="更新时间"  />-->

