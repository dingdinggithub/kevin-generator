<?xml version="1.0" encoding="UTF-8"?>                                          
<!DOCTYPE entity SYSTEM "../../dtd/table.dtd">
<entity name="AuthModule" table="t_mem_auth_module" description="权限模块关系" author="丁海峰" since="1.0">
	<!--
	    主键
	 -->
	<id name="id" column="id" type="long" description="主键"/>

	<property	name="code"   column="code" type="string"  maxlength="20"   description="权限码"  />
	<property	name="name"   column="name" type="string"  maxlength="20"   description="权限名称"  />
	<property	name="moduleId"   column="module_id" type="long"  maxlength="20"   description="权限所在的模块id"  />
	<property	name="url"   column="url" type="string"  maxlength="100"   description="请求的url"  />
	<property	name="type"   column="type" type="int"  maxlength="11"   description="类型，1：菜单，2：按钮，3：其他"  />
	<property	name="seq"   column="seq" type="int"  maxlength="11"   description="权限模块在当前层级下的顺序，由小到大"  />
	<property	name="status"   column="status" type="int"  maxlength="11"   description="状态，1：正常，0：冻结"  />

