<?xml version="1.0" encoding="UTF-8"?>                                          
<!DOCTYPE entity SYSTEM "../../dtd/table.dtd">
<entity name="sjxx" table="sjxx" description="学院事件信息统计表" author="牛鑫" since="1.0">
	<!--                                                                          
		主键                                                                        
	 -->                                                                          
	<id name="id" column="id" type="string" description="主键"/>
<property	name="SJMC"  		 column="SJMC" type="string"  maxlength="50" description="事件名称"   />
<property	name="SJRW"  		 column="SJRW" type="string"  maxlength="20"     description="参加事件人物"   />
<property	name="SJDD"      		 column="SJDD" type="string"  maxlength="20"   description="事件发生地点"     />
<property	name="SJBM"     		 column="SJBM" type="string"  maxlength="20"   description="举办部门"     />
<property	name="SJRQ"      		 column="SJRQ" type="date"  maxlength="0"  description="事件发生日期" dateFmt="yyyy-MM-dd"      />
<property	name="SJXNXQ"  		 column="SJXNXQ" type="string"  maxlength="50"  description="学年学期"    />
</entity>        